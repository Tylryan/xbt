

rule Entry {
    $var = "Me" .

    /* Referencing variables within shell commands
     * do work.
     * This would print 'Me'.*/

    $ echo $var
    /* However, referencing Rule Members doesn't
     * seem to work. 
     * This would print 'Branch::'. 
     */
    $ echo You::$cmd
}

rule You { $cmd = "echo You" . }