
$PROJECT_DIR = "examples/c_project" .

rule Entry {
    build_files: "${PROJECT_DIR}/main.c"
                 Helper::$output_files    .
    $ echo $build_files
}

rule Helper {
    build_files: "${PROJECT_DIR}/helper.c"  .
    output_files: "${PROJECT_DIR}/helper.o" .

    $ gcc -o $output_files -c $build_files
}
